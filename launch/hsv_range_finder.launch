<!--defining the area of the cells in the game-->
<launch>
  <include file="$(find panda_tictactoe)/launch/usb_cam_node.launch" />

  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find panda_tictactoe)/custom_rosconsole.conf"/>

  <node name="hsv_range_finder" pkg="panda_tictactoe" type="hsv_range_finder" respawn="false" output="screen">
    <remap from="/image_in" to="/usb_cam/image_raw"/>
  </node>

</launch>

